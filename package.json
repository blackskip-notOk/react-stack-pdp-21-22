		"jest-environment-jsdom": "^28.1.3",
		"jest-fetch-mock": "^3.0.3",
		"jest-watch-typeahead": "^2.0.0",
		"jsdom": "^20.0.0",
		"less": "^4.1.2",
		"lint-staged": "^13.0.3",
		"msw": "^0.44.2",
		"postcss-less": "^6.0.0",
		"pre-commit": "^1.2.2",
		"prettier": "2.6.2",
		"pretty-quick": "^3.1.3",
		"react-test-renderer": "^18.1.0",
		"stylelint": "^14.10.0",
		"stylelint-config-prettier": "^9.0.3",
		"stylelint-config-standard": "^27.0.0",
		"ts-jest": "^28.0.8",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"vite": "^3.0.7"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "test"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"yarn run lint:write",
			"yarn run prettier"
		]
	},
	"test": "NODE_ENV=test jest"
}
