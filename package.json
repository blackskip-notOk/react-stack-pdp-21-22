{
	"name": "pdp-2022",
	"private": true,
	"version": "0.0.1",
	"description": "React based project for pdp-2022",
	"repository": "https://github.com/blackskip-notOk/react-stack-pdp-21-22",
	"homepage": "https://pdp-blackskip.vercel.app",
	"author": "Ilya Babanov <ilbabanov@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "vite --open http://localhost:3000/",
		"build": "tsc -p tsconfig.prod.json && vite build",
		"preview": "vite preview --port 3003",
		"prepare": "husky install",
		"test": "jest --watch --logHeapUsage",
		"test:update": "jest --updateSnapshot",
		"test:coverage": "jest --coverage=true",
		"test:commit": "jest --lastCommit --logHeapUsage",
		"test:clear": "jest --clearCache",
		"lint": "eslint --debug src/",
		"lint:write": "eslint --debug src/ --fix",
		"prettier": "prettier --write src/**/*.{ts,tsx}",
		"lint:styles": "stylelint \"src/**/*.(css|less)\"",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@emotion/react": "^11.10.0",
		"@emotion/styled": "^11.10.0",
		"@hookform/resolvers": "^2.8.8",
		"@mui/icons-material": "^5.6.2",
		"@mui/lab": "^5.0.0-alpha.79",
		"@mui/material": "^5.6.3",
		"@mui/styled-engine-sc": "^5.6.1",
		"@react-rxjs/core": "^0.9.6",
		"@react-rxjs/utils": "^0.9.5",
		"@reduxjs/toolkit": "^1.8.2",
		"@types/ramda": "^0.28.7",
		"axios": "^0.26.1",
		"i18next": "^21.8.14",
		"i18next-browser-languagedetector": "^6.1.4",
		"i18next-http-backend": "^1.4.1",
		"luxon": "^3.0.1",
		"node-notifier": "^10.0.1",
		"path": "^0.12.7",
		"postcss": "^8.4.12",
		"ramda": "^0.28.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-error-boundary": "^3.1.4",
		"react-hook-form": "^7.29.0",
		"react-i18next": "^11.18.1",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"rxjs": "^7.5.6",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@reactivex/rxjs": "^6.6.7",
		"@swc/cli": "^0.1.57",
		"@swc/core": "^1.2.239",
		"@swc/jest": "^0.2.22",
		"@testing-library/jest-dom": "^5.16.5",
		"@testing-library/react": "^13.1.1",
		"@testing-library/react-hooks": "^8.0.1",
		"@testing-library/user-event": "^14.0.4",
		"@types/jest": "^28.1.7",
		"@types/luxon": "^3.0.0",
		"@types/react": "^18.0.8",
		"@types/react-dom": "^18.0.0",
		"@types/react-test-renderer": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"@vitejs/plugin-react": "^1.3.0",
		"c8": "^7.12.0",
		"camelcase": "6",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jest-dom": "^4.0.2",
		"eslint-plugin-jsx-a11y": "^6.5.1",
