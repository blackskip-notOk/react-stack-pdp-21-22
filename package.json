{
	"name": "pdp-2022",
	"private": true,
	"version": "0.0.1",
	"description": "React based project for pdp-2022",
	"repository": "https://github.com/blackskip-notOk/react-stack-pdp-21-22",
	"author": "Ilya Babanov <ilbabanov@gmail.com>",
	"license": "MIT",
	"scripts": {
		"start": "vite --open",
		"build": "tsc && vite build",
		"preview": "vite preview",
		"prepare": "husky install",
		"test": "jest --bail --runInBand --coverage=false",
		"lint": "eslint --debug src/",
		"lint:write": "eslint --debug src/ --fix",
		"prettier": "prettier --write src/**/*.{ts,tsx}",
		"postinstall": "husky install",
		"test:commit": "jest --bail --lastCommit --logHeapUsage --coverage=false",
		"test:push": "jest --bail --logHeapUsage --coverage=true"
	},
	"dependencies": {
		"@emotion/react": "^11.9.0",
		"@emotion/styled": "^11.8.1",
		"@hookform/resolvers": "^2.8.8",
		"@mui/icons-material": "^5.6.2",
		"@mui/lab": "^5.0.0-alpha.79",
		"@mui/material": "^5.6.3",
		"@mui/styled-engine-sc": "^5.6.1",
		"@react-rxjs/core": "^0.9.6",
		"@react-rxjs/utils": "^0.9.5",
		"@types/ramda": "^0.28.7",
		"axios": "^0.26.1",
		"effector": "^22.2.0",
		"effector-localstorage": "^0.5.3",
		"effector-react": "^22.0.6",
		"effector-storage": "^5.0.0",
		"effector-utils": "^0.1.0",
		"node-notifier": "^10.0.1",
		"path": "^0.12.7",
		"postcss": "^8.4.12",
		"ramda": "^0.28.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-hook-form": "^7.29.0",
		"react-router-dom": "^6.3.0",
		"react-spinners": "^0.11.0",
		"rxjs": "^7.5.5",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@reactivex/rxjs": "^6.6.7",
		"@testing-library/jest-dom": "^5.16.3",
		"@testing-library/react": "^13.1.1",
		"@testing-library/user-event": "^14.0.4",
		"@types/jest": "^27.4.1",
		"@types/react": "^18.0.8",
		"@types/react-dom": "^18.0.0",
		"@types/react-test-renderer": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"@vitejs/plugin-react": "^1.3.0",
		"effector-logger": "^0.13.4",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-effector": "^0.6.0",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jest": "^27.5.1",
		"jest-circus": "^27.5.1",
		"jest-watch-typeahead": "^1.0.0",
		"less": "^4.1.2",
		"msw": "^0.39.2",
		"pre-commit": "^1.2.2",
		"prettier": "2.6.2",
		"pretty-quick": "^3.1.3",
		"react-test-renderer": "^18.1.0",
		"ts-jest": "^27.1.4",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"vite": "^2.9.1"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "test"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"yarn run lint:write",
			"yarn run prettier"
		]
	}
}
