{
	"name": "pdp-2022",
	"private": true,
	"version": "0.0.1",
	"description": "React based project for pdp-2022",
	"repository": "https://github.com/blackskip-notOk/react-stack-pdp-21-22",
	"homepage": "https://pdp-blackskip.vercel.app",
	"author": "Ilya Babanov <ilbabanov@gmail.com>",
	"license": "MIT",
	"scripts": {
		"dev": "vite --open http://localhost:3000/",
		"build": "tsc && vite build",
		"preview": "vite preview --port 3003",
		"prepare": "husky install",
		"test": "vitest --watch --ui",
		"test:update": "vitest --update",
		"test:coverage": "vitest run --coverage",
		"test:no-watch": "vitest run",
		"lint": "eslint --debug src/",
		"lint:write": "eslint --debug src/ --fix",
		"prettier": "prettier --write src/**/*.{ts,tsx}",
		"postinstall": "husky install"
	},
	"dependencies": {
		"@emotion/react": "^11.10.0",
		"@emotion/styled": "^11.10.0",
		"@hookform/resolvers": "^2.8.8",
		"@mui/icons-material": "^5.6.2",
		"@mui/lab": "^5.0.0-alpha.79",
		"@mui/material": "^5.6.3",
		"@mui/styled-engine-sc": "^5.6.1",
		"@react-rxjs/core": "^0.9.6",
		"@react-rxjs/utils": "^0.9.5",
		"@reduxjs/toolkit": "^1.8.2",
		"@types/ramda": "^0.28.7",
		"axios": "^0.26.1",
		"i18next": "^21.8.14",
		"i18next-browser-languagedetector": "^6.1.4",
		"i18next-http-backend": "^1.4.1",
		"luxon": "^3.0.1",
		"node-notifier": "^10.0.1",
		"path": "^0.12.7",
		"postcss": "^8.4.12",
		"ramda": "^0.28.0",
		"react": "^18.0.0",
		"react-dom": "^18.0.0",
		"react-error-boundary": "^3.1.4",
		"react-hook-form": "^7.29.0",
		"react-i18next": "^11.18.1",
		"react-redux": "^8.0.2",
		"react-router-dom": "^6.3.0",
		"rxjs": "^7.5.6",
		"yup": "^0.32.11"
	},
	"devDependencies": {
		"@reactivex/rxjs": "^6.6.7",
		"@testing-library/react": "^13.1.1",
		"@testing-library/user-event": "^14.0.4",
		"@types/luxon": "^3.0.0",
		"@types/react": "^18.0.8",
		"@types/react-dom": "^18.0.0",
		"@types/react-test-renderer": "^18.0.0",
		"@typescript-eslint/eslint-plugin": "^5.17.0",
		"@typescript-eslint/parser": "^5.17.0",
		"@vitejs/plugin-react": "^1.3.0",
		"@vitest/coverage-c8": "^0.22.0",
		"@vitest/ui": "^0.21.1",
		"c8": "^7.12.0",
		"eslint": "^8.12.0",
		"eslint-config-prettier": "^8.5.0",
		"eslint-import-resolver-alias": "^1.1.2",
		"eslint-plugin-import": "^2.25.4",
		"eslint-plugin-jsx-a11y": "^6.5.1",
		"eslint-plugin-prettier": "^4.0.0",
		"eslint-plugin-react": "^7.29.4",
		"eslint-plugin-react-hooks": "^4.4.0",
		"eslint-plugin-simple-import-sort": "^7.0.0",
		"husky": "^7.0.0",
		"identity-obj-proxy": "^3.0.0",
		"jsdom": "^20.0.0",
		"less": "^4.1.2",
		"msw": "^0.44.2",
		"pre-commit": "^1.2.2",
		"prettier": "2.6.2",
		"pretty-quick": "^3.1.3",
		"react-test-renderer": "^18.1.0",
		"ts-node": "^10.7.0",
		"typescript": "^4.6.3",
		"vite": "^3.0.7",
		"vitest": "^0.22.0"
	},
	"husky": {
		"hooks": {
			"pre-commit": "lint-staged",
			"pre-push": "test"
		}
	},
	"lint-staged": {
		"src/**/*.{ts,tsx}": [
			"yarn run lint:write",
			"yarn run prettier"
		]
	}
}
